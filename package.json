{
  "name": "DrYnks",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --dev-client --tunnel -c",
    "start:client": "expo start --dev-client -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:choose": "expo run:ios --device",
    "ios:pod": "npx pod-install ios",
    "clean:ios": "rm -rf ios/build ios/Pods ios/Podfile.lock && npx pod-install ios",
    "reset:metro": "bash -lc 'pkill -f \"node.*metro\" 2>/dev/null || true; watchman watch-del-all 2>/dev/null || true; rm -rf $TMPDIR/metro-* $TMPDIR/haste-map-* 2>/dev/null || true'",
    "clean:metro": "watchman watch-del-all || true && rm -rf $TMPDIR/metro-* $TMPDIR/haste-* .expo .expo-shared node_modules/.cache",
    "web": "expo start --web",
    "doctor": "expo doctor",
    "handoff": "bash scripts/handoff.sh",
    "typecheck": "tsc -p tsconfig.json",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "codemod:invites:preview": "node scripts/preview-host-invites.mjs",
    "codemod:invites": "node scripts/rename-invites-host-to-inviter.mjs",
    "postinstall": "patch-package || true"
  },
  "dependencies": {
    "@expo/config-plugins": "10.1.1",
    "@expo/vector-icons": "14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "6.5.11",
    "@react-navigation/native": "6.1.14",
    "@react-navigation/native-stack": "6.9.21",
    "@sendgrid/mail": "8.1.5",
    "@supabase/supabase-js": "2.50.2",
    "axios": "1.11.0",
    "base-64": "^1.0.0",
    "date-fns-tz": "3.2.0",
    "expo": "53.0.17",
    "expo-apple-authentication": "7.2.4",
    "expo-av": "15.1.7",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "0.14.8",
    "expo-clipboard": "7.1.5",
    "expo-constants": "17.1.7",
    "expo-crypto": "14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-device": "7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "14.1.5",
    "expo-linking": "7.1.7",
    "expo-local-authentication": "~16.0.5",
    "expo-location": "~18.1.6",
    "expo-modules-core": "2.5.0",
    "expo-notifications": "0.31.4",
    "expo-secure-store": "14.2.3",
    "expo-sharing": "13.1.5",
    "expo-splash-screen": "0.30.10",
    "expo-status-bar": "2.2.3",
    "expo-system-ui": "5.0.10",
    "expo-updates": "^0.28.17",
    "formik": "2.4.2",
    "geolib": "3.3.4",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-branch": "^6.8.0",
    "react-native-draggable-flatlist": "4.0.3",
    "react-native-emoji-selector": "0.2.0",
    "react-native-gesture-handler": "2.24.0",
    "react-native-get-random-values": "1.11.0",
    "react-native-google-places-autocomplete": "2.5.7",
    "react-native-linear-gradient": "2.8.3",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.11.1",
    "react-native-shimmer-placeholder": "2.0.9",
    "react-native-svg": "15.11.2",
    "react-native-swipe-list-view": "3.2.9",
    "react-native-url-polyfill": "2.0.0",
    "react-native-vector-icons": "9.2.0",
    "recoil": "0.7.7",
    "tz-lookup": "6.1.25",
    "uuid": "^11.1.0",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@types/react": "19.0.10",
    "@types/uuid": "10.0.0",
    "babel-plugin-dotenv-import": "3.0.1",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-transform-remove-console": "6.9.4",
    "dotenv": "^17.2.2",
    "eslint": "8.56.0",
    "expo-modules-autolinking": "2.1.14",
    "patch-package": "8.0.0",
    "prettier": "3.2.5",
    "typescript": "5.3.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "expo-updates",
        "expo-splash-screen"
      ]
    }
  }
}
