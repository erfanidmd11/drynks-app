{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "local"
  },

  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_DISABLE_BIOMETRICS": "1",
        "EXPO_PUBLIC_DISABLE_PUSH": "1",
        "EXPO_PUBLIC_SAFE_BOOT": "1"
      }
    },

    "staging": {
      "developmentClient": false,
      "distribution": "internal",
      "channel": "staging",
      "env": {
        "EXPO_PUBLIC_DISABLE_BIOMETRICS": "1",
        "EXPO_PUBLIC_DISABLE_PUSH": "1",
        "EXPO_PUBLIC_SAFE_BOOT": "1"
      }
    },

    "production": {
      "developmentClient": false,
      "distribution": "store",
      "channel": "production",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_DISABLE_BIOMETRICS": "1",
        "EXPO_PUBLIC_DISABLE_PUSH": "0",
        "EXPO_PUBLIC_SAFE_BOOT": "1"
      }
    }
  },

  "submit": {
    "production": {
      "ios": {
        "appleId": "admin@pivot.do",
        "ascAppId": "6747322650",
        "appleTeamId": "SUFCY3Z3B4"
      }
    }
  }
}
